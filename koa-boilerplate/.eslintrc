{
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": false,
    "node": false,
    "amd": false,
    "mocha": false,
    "jasmine": false,
    "es6": false
  },
  "globals": {
    "__dirname": false,
    "__filename": false,
    "require": false,
    "module": false,
    "console": false,
    "formatters": true,
    "process": false
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
    "react",
    "import",
    "prefer-object-spread"
  ],
  "rules": {
    "semi": [2, "never"],
    "semi-spacing": [2, {"before": false,"after": true}],
    "generator-star-spacing": 0,
    "no-param-reassign": ["error", { "props": false }],
    "import/prefer-default-export": 0,
    "no-console": 0,
    "import/no-extraneous-dependencies": [
      "error", {
        "devDependencies": true
      }
    ]
  }
}