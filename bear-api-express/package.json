{
  "name": "bear-api-express",
  "version": "0.0.1",
  "description": "Express 项目模板",
  "main": "src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon server/start.js --watch server --exec babel-node",
    "start:prod": "cross-env NODE_ENV=production node build/server",
    "clean": "rimraf build",
    "copy:server": "copyfiles server/**/*.json server/**/*.html build",
    "build:client": "cross-env NODE_ENV=production webpack -p --config webpack.config.js --progress",
    "build:server": "cross-env NODE_ENV=production babel server -d build/server && npm run copy:server",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "create-tables": "cross-env NODE_ENV=development babel-node server/createTables.js",
    "recreate-tables": "cross-env NODE_ENV=development babel-node server/createTables.js --force"
  },
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "lodash": "^4.17.4",
    "axios": "^0.16.2",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.5.2",
    "consolidate": "^0.14.5",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "express-formidable": "1.0.0",
    "express-jwt": "^5.1.0",
    "express-session": "^1.15.3",
    "formidable": "^1.1.1",
    "method-override": "^2.3.9",
    "history": "^4.5.0",
    "connect-redis": "^3.3.0",
    "mongoose": "^4.2.9",
    "nunjucks": "^3.0.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "resource-router-middleware": "^0.5.1",
    "knex": "^0.13.0",
    "objection": "^0.8.4",
    "mysql": "^2.13.0",
    "ioredis": "^3.1.1",
    "graphql": "^0.10.3",
    "graphql-server-express": "^0.9.0",
    "jsonwebtoken": "^7.4.1",
    "winston": "^2.1.1",
    "mkdirp": "^0.5.1",
    "object-path": "^0.11.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-import": "^2.7.0",
    "nodemon": "^1.11.0",
    "cross-env": "^5.0.1",
    "rimraf": "^2.6.1",
    "copyfiles": "^1.2.0",
    "supertest": "^3.0.0",
    "faker": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thoughtbit/node-web-starter/bear-api-express.git"
  },
  "author": {
    "name": "moocss",
    "email": "moocss@gmail.com"
  },
  "engines": {
    "node": ">= 6.9"
  },
  "license": "MIT"
}
